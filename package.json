{
  "name": "mytools-zack",
  "displayName": "空山工具箱",
  "description": "some tools personal used",
  "repository": "https://github.com/CodeByZack/mytools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mytools-zack.json2ts",
    "onCommand:mytools-zack.translate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mytools-zack.json2ts",
        "title": "json2ts"
      },
      {
        "command": "mytools-zack.translate",
        "title": "translate"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "mytools-zack.json2ts",
          "group": "navigation"
        },
        {
          "command": "mytools-zack.translate",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "json2tstool": "^1.0.7"
  }
}
